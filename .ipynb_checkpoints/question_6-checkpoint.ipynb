    "import numpy as np\n",
    "\n",
    "def arrays_cov(a, b):\n",
    "    if isinstance(a, np.ndarray) and isinstance(b, np.ndarray):\n",
    "        return np.cov(a, b)\n",
    "    else:\n",
    "        return \"Error\"\n",
    "    \n",
    "y = np.array([0,1,2])\n",
    "z = np.array([2,1,0])\n",
    "\n",
    "print(arrays_cov(y, z))"
   ]
